{
    "name": "divineniiquaye/php-rade",
    "description": "A fast micro, compilable, and scalable PHP framework",
    "keywords": [
        "framework",
        "rade",
        "divinenii",
        "php-framework",
        "php framework",
        "framework",
        "php",
        "php-17",
        "psr-15",
        "psr-11",
        "psr-7"
    ],
    "homepage": "https://www.divienii.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        }
    ],
    "support": {
        "docs": "https://docs.divienii.com/php-rade",
        "issues": "https://github.com/divineniiquaye/php-rade/issues",
        "rss": "https://github.com/divineniiquaye/php-rade/releases.atom",
        "source": "https://github.com/divineniiquaye/php-rade"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "biurad/http-galaxy": "1.*",
        "divineniiquaye/flight-routing": "^1.6",
        "divineniiquaye/rade-di": "dev-rade-best",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/event-dispatcher-contracts": "^2|^3",
        "symfony/finder": "^5.4 || ^6.0",
        "symfony/process": "^5.4 || ^6.0"
    },
    "require-dev": {
        "biurad/annotations": "1.*",
        "biurad/cache": "1.*",
        "biurad/security": "1.*",
        "biurad/templating": "1.*",
        "doctrine/annotations": "^1.12",
        "latte/latte": "^2.10",
        "league/flysystem": "^2.0 || ^3.0",
        "monolog/monolog": "^2",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.1",
        "spiral/attributes": "^2.8",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/asset": "^5.4 || ^6.0",
        "symfony/cache": "^5.4 || ^6.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/http-client": "^5.4 || ^6.0",
        "symfony/mailer": "^5.4 || ^6.0",
        "symfony/notifier": "^5.4 || ^6.0",
        "symfony/rate-limiter": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/translation": "^5.4 || ^6.0",
        "symfony/twig-bridge": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "symfony/var-dumper": "^5.4 || ^6.0",
        "symfony/workflow": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "tracy/tracy": "^2.8",
        "vimeo/psalm": "^4.10",
        "webonyx/graphql-php": "^14.11"
    },
    "suggest": {
        "biurad/annotations": "A powerful annotations and attributes support for PHP 7.2+",
        "biurad/cache": "A powerful cache library for advanced caching using PSR-6 or PSR-16",
        "league/flysystem": "A FileManager for handling filesystem on local disk and cloud storage",
        "biurad/templating": "A library that provides all the tools needed in working with any kind of template system",
        "tracy/tracy": "A library that makes PHP exceptions look friendly, and provides debugging support"
    },
    "autoload": {
        "psr-4": {
            "Rade\\": "src/",
            "Rade\\DI\\Extensions\\": "src/Extensions"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rade\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "phpcs": "phpcs -q",
        "phpstan": "phpstan analyse",
        "psalm": "psalm --show-info=true",
        "phpunit": "phpunit --no-coverage",
        "test": [
            "@phpcs",
            "@phpstan",
            "@psalm",
            "@phpunit"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
