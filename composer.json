{
    "name": "biurad/flange",
    "description": "A fast micro, compilable, and scalable PHP framework",
    "keywords": [
        "framework",
        "flange",
        "divinenii",
        "php-framework",
        "php framework",
        "framework",
        "php",
        "php-17",
        "psr-15",
        "psr-11",
        "psr-7"
    ],
    "homepage": "https://biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
          "name": "Divine Niiquaye Ibok",
          "email": "divineibok@gmail.com"
        },
        {
          "name": "GitHub Contributors",
          "homepage": "https://github.com/biurad/flange/contributors"
        }
    ],
    "support": {
        "docs": "https://docs.biurad.com/php/flange"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "biurad/http-galaxy": "1.*",
        "divineniiquaye/flight-routing": "2.*",
        "divineniiquaye/rade-di": "1.*",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/event-dispatcher-contracts": "^3",
        "symfony/finder": "^6.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "biurad/annotations": "1.*",
        "biurad/security": "1.*",
        "biurad/templating": "1.*",
        "doctrine/annotations": "^1.12",
        "latte/latte": "^2.10",
        "league/flysystem": "^2.0 || ^3.0",
        "monolog/monolog": "^2",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.1",
        "spiral/attributes": "^2.8",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/asset": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/form": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/lock": "6.0",
        "symfony/mailer": "^6.0",
        "symfony/notifier": "^6.0",
        "symfony/rate-limiter": "^6.0",
        "symfony/security-csrf": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bridge": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/workflow": "^6.0",
        "symfony/yaml": "^6.0",
        "tracy/tracy": "^2.8",
        "webonyx/graphql-php": "^14.11",
        "cycle/orm": "^2.0",
        "cycle/annotated": "^3.1",
        "cycle/migrations": "^3.0",
        "cycle/schema-migrations-generator": "^2.0",
        "doctrine/dbal": "^3.1",
        "doctrine/persistence": "^2.2",
        "doctrine/sql-formatter": "^1.0.1"
    },
    "suggest": {
        "biurad/annotations": "A powerful annotations and attributes support for PHP 7.2+",
        "biurad/cache": "A powerful cache library for advanced caching using PSR-6 or PSR-16",
        "league/flysystem": "A FileManager for handling filesystem on local disk and cloud storage",
        "biurad/templating": "A library that provides all the tools needed in working with any kind of template system",
        "nikic/php-parser": "A library for building up cache support to performance",
        "tracy/tracy": "A library that makes PHP exceptions look friendly, and provides debugging support"
    },
    "autoload": {
        "psr-4": {
            "Flange\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flange\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "phpcs": "phpcs -q",
        "phpstan": "phpstan analyse",
        "phpunit": "phpunit --no-coverage",
        "test": [
            "@phpcs",
            "@phpstan",
            "@phpunit"
        ]
    }
}
